            return ;
        }

        systemNoticeDetails(rows[0]._id)
    }
    //系统公告详情
    function systemNoticeDetails(id){

        $.ajax({
            url: '{{projcfg.appurl}}/admin/api/common/syscfg/notice/'+id,
            type: 'get',
            data: {},
            success: function (data) {

                if(data.success) {
                    var html = '';
                    if(data.data!=''&&data.data!=null){
                        var type;
                        var noticeType = data.data[0].notice_type;
                        if(noticeType == 1){
