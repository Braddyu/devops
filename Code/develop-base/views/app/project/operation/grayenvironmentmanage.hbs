<script>
    let num=1;
    let all=0;
    $(document).ready(function () {

        $('.next-deploy').click(function () {

            if(num==2){
                $("#progress").html("67%");
                $("#progress").css("width","67%");
                $("#td2").html("develop-base:v1.4.4");
                    $('.deploy-state div:eq(1)').removeClass('d-cancel').addClass('deploy-antime');

            }
            if(num==1){
                    $("#progress").html("33%");
                    $("#progress").css("width","33%");
                    $("#td1").html("develop-base:v1.4.4");
                    $('.deploy-state div:eq(0)').removeClass('d-cancel').addClass('deploy-antime');

            }if(num==3){
                $("#td3").html("develop-base:v1.4.4");
                    $("#progress").html("100%");
                    $("#progress").css("width","100%");
                    $('.deploy-state div:eq(2)').removeClass('d-cancel').addClass('deploy-antime');

            }
            if(num==4){
                all=1;
                $('.d-step-2').hide();
                $('.d-step-3').show();
                num=0;
            }
            num+=1;


        });


        $('.deploy-cancel').click(function () {
            num=1;
            all=0;
            $("#progress").html("0%");
            $("#progress").css("width","0%");
            $("#td3").html("develop-base:v1.4.3");
            $("#td1").html("develop-base:v1.4.3");
            $("#td2").html("develop-base:v1.4.3");
            $('.deploy-state div').removeClass('deploy-antime').addClass('d-cancel');
        });
        $('#next-step').click(function () {
            $('.d-step-1').hide();
            $('.d-step-2').show();
        });
        $('.all-deploy').click(function () {
            $("#progress").html("100%");
            $("#progress").css("width","100%");
            $('.deploy-state div').removeClass('d-cancel').addClass('deploy-antime');
            num=4;
            if(all==1||num==4){
                $('.d-step-2').hide();
            $('.d-step-3').show();
            }
            all+=1;

        });
    })
</script>

