        -moz-animation:cancel 2s linear forwards;
        /* Safari and Chrome: */
        -webkit-animation:cancel 2s linear forwards;
    }

    @keyframes cancel
    {
        0%   {left:0}
        100% {left:-100%}
    }

    @-moz-keyframes cancel /* Firefox */
    {
        0%   {left:0}
        100% {left:-100%}
