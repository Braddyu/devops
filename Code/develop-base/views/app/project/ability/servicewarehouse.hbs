<script  type="text/javascript">






    function doAdd(value) {
        var validate = $('#serviceForm').form('validate');
        if(!validate) {
            return false;
        }
        closeDialog();
        window.location.href = "{{projcfg.appurl}}/servicem";
    }
    function to_detail(){
        window.location.href = "{{projcfg.appurl}}/servicedetail";
    }

    // 打开页面
    function openPage(title, value, callback) {
        $("#serviceName").html(value);
        value = '';
        $('#myModal').show();
        $('#myModal').mydialog({
            title:title,
            width: 700,
            height: 485,
            top:100,
            modal: true,
            myButtons:[
                {
                    text:'创建',
                    btnCls:'btn btn-blue',
                    handler:function(){
                        callback(value);
                    }
                },
                {
                    text:'关闭',
                    btnCls:'btn btn-danger',
                    handler:function(){
                        closeDialog();
                    }
                }
            ]
        });
    }

    // 清空新增表单数据
    function clear() {
        $('#serviceForm').form('clear');
    }

    // 关闭窗口
    function closeDialog() {
        $('#myModal').dialog('close');
        clear();
    }

</script>