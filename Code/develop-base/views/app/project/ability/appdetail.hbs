        }else{
            //禁用'-'按钮
            $("#sub_"+service_id).attr("disabled",true);
            $.ajax({
                url:'{{projcfg.appurl}}/api/project/ability/appdetail/delContainer',
                type:'get',
                data: {'service_id':service_id},
                success:function(data){
                    console.log(data);
                    //注:因为每次操作都是调用外部应用进行操作，所以数据都是全新的，这里做重新显示
